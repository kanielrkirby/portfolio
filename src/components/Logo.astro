---
import { Image } from "@astrojs/image/components";
import logo from "@/assets/svgs/logo.svg";
export interface Props {
  class?: string;
}

const { class: className = "" } = Astro.props;
---

<Image
  {...Astro.props}
  src={logo}
  alt="Logo of a K"
  class={`rounded-xl transition-all duration-200 ${className}`}
/>
