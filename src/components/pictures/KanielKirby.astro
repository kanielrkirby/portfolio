---
import { Image } from "astro:assets";
import kanielKirby from "@/assets/images/kaniel-kirby-1.webp";
---

<Image
  src={kanielKirby}
  alt=""
  class="h-full w-full object-cover"
  format="avif"
  loading="eager"
  decoding="sync"
  sizes="(max-width: 400px) 400w, (max-width: 800px) 800w, (max-width: 1200px) 1200w, (max-width: 1800px) 1800w"
  {...Astro.props}
/>
